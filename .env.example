# Backend Environment Configuration
# Copy this to .env and fill in your values

# Flask Configuration
FLASK_ENV=production
SECRET_KEY=your-secret-key-here-change-in-production
DEBUG=True
PORT=8000
HOST=0.0.0.0

# CORS Configuration
CORS_ORIGINS=https://agriculture-frontend-c5jkss8vr-rants-projects-a8fe1fad.vercel.app/auth

# Oracle Database Configuration
ORACLE_USER=AGRICULTURE
ORACLE_PASSWORD=Rantdev
ORACLE_DSN=localhost:1521/XE
ORACLE_POOL_MIN=1
ORACLE_POOL_MAX=10
ORACLE_POOL_INCREMENT=1
ORACLE_TIMEOUT=30

# ML Models Configuration
ENABLE_MODEL_CACHE=true

# Features
ENABLE_ORACLE=true
ENABLE_CACHE=true
ENABLE_ASYNC=false

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/backend.log

# Sentry Error Tracking (optional)
# SENTRY_DSN=https://your-sentry-key@sentry.io/project-id
# copy example to .env (do this once)
Copy-Item "Backend\\.env.example" "Backend\\.env" -Force

# show the Oracle lines to confirm values
Get-Content "Backend\\.env" | Select-String -Pattern "ORACLE_USER|ORACLE_PASSWORD|ORACLE_DSN|ENABLE_ORACLE"